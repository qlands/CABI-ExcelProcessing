      <sheets>
        <sheet name="Maize Harvest" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="8" processorReference="harvest-maize-processor" requireAll="true">
