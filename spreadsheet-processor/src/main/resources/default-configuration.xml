<?xml version="1.0" encoding="UTF-8"?>
<processor-configuration>
  <cell-processors>
    <cell-processor name="valset" class="org.cabi.ofra.dataload.impl.ValidateAndSet"/>
    <cell-processor name="checkPresent" class="org.cabi.ofra.dataload.impl.CheckPresent"/>
    <cell-processor name="trialValidator" class="org.cabi.ofra.dataload.impl.TrialValidator"/>
    <cell-processor name="trialSoilSampleValidator" class="org.cabi.ofra.dataload.impl.TrialSoilSampleValidator"/>
    <cell-processor name="blockValidator" class="org.cabi.ofra.dataload.impl.BlockValidator"/>
    <cell-processor name="blockSoilSampleValidator" class="org.cabi.ofra.dataload.impl.BlockSoilSampleValidator"/>
    <cell-processor name="plotValidator" class="org.cabi.ofra.dataload.impl.PlotValidator"/>
    <cell-processor name="plotPlantSampleValidator" class="org.cabi.ofra.dataload.impl.PlotPlantSampleValidator"/>
    <cell-processor name="plotSoilSampleValidator" class="org.cabi.ofra.dataload.impl.PlotSoilSampleValidator"/>
    <cell-processor name="harvestValidator" class="org.cabi.ofra.dataload.impl.HarvestValidator"/>
  </cell-processors>
  <range-processors>
    <!-- Trial Processors -->
    <range-processor name="trial-blocks-processor" class="org.cabi.ofra.dataload.specific.TrialDefinitionTemplate$BlockRangeProcessor"/>
    <range-processor name="trial-plots-processor" class="org.cabi.ofra.dataload.specific.TrialDefinitionTemplate$PlotRangeProcessor"/>
    <range-processor name="trial-soil-sample-processor" class="org.cabi.ofra.dataload.specific.TrialSoilTemplates$TrialSoilSampleRangeProcessor"/>
    <range-processor name="trial-soil-result-processor" class="org.cabi.ofra.dataload.specific.TrialSoilTemplates$TrialSoilResultRangeProcessor"/>
    <!-- Block Processors -->
    <range-processor name="block-activities-processor" class="org.cabi.ofra.dataload.specific.BlockTemplates$BlockActivitiesRangeProcessor"/>
    <range-processor name="block-observations-processor" class="org.cabi.ofra.dataload.specific.BlockTemplates$BlockObservationsRangeProcessor"/>
    <range-processor name="block-soil-sample-processor" class="org.cabi.ofra.dataload.specific.BlockTemplates$BlockSoilSampleRangeProcessor"/>
    <range-processor name="block-soil-result-processor" class="org.cabi.ofra.dataload.specific.BlockTemplates$BlockSoilResultRangeProcessor"/>
    <range-processor name="block-weather-processor" class="org.cabi.ofra.dataload.specific.BlockTemplates$BlockWeatherRangeProcessor"/>
    <!-- Plot Processors -->
    <range-processor name="plot-activities-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotActivitiesRangeProcessor"/>
    <range-processor name="plot-observations-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotObservationsRangeProcessor"/>
    <range-processor name="plot-plant-sample-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotPlantSampleRangeProcessor"/>
    <range-processor name="plot-plant-sample-results-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotPlantSampleResultsRangeProcessor"/>
    <range-processor name="plot-soil-sample-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotSoilSampleRangeProcessor"/>
    <range-processor name="plot-soil-sample-results-processor" class="org.cabi.ofra.dataload.specific.PlotTemplates$PlotSoilSampleResultsRangeProcessor"/>
    <!-- Harvest Processors: Legumes -->
    <range-processor name="harvest-legumes-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestLegumeRangeProcessor"/>
    <range-processor name="harvest-legumes-moisture-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestLegumeMoistureRangeProcessor"/>
    <range-processor name="harvest-legumes-flowering-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestLegumeFloweringRangeProcessor"/>
    <!-- Harvest Processors: Cassava -->
    <range-processor name="harvest-cassava-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCassavaRangeProcessor"/>
    <range-processor name="harvest-cassava-weights-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCassavaDryWeightsRangeProcessor"/>
    <!-- Harvest Processors: Maize -->
    <range-processor name="harvest-maize-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestMaizeRangeProcessor"/>
    <range-processor name="harvest-maize-moisture-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestMaizeMoistureRangeProcessor"/>
    <range-processor name="harvest-maize-silkdate-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestMaizeSilkDateRangeProcessor"/>
    <range-processor name="harvest-maize-dryweight-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestMaizeStoverDryWeightRangeProcessor"/>
    <!-- Harvest Processors: Cereals -->
    <range-processor name="harvest-cereals-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCerealRangeProcessor"/>
    <range-processor name="harvest-cereals-bootingdate-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCerealBootingDateRangeProcessor"/>
    <range-processor name="harvest-cereals-moisture-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCerealMoistureContentRangeProcessor"/>
    <range-processor name="harvest-cereals-dryweight-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestCerealStoverDryRangeProcessor"/>
    <!-- Harvest Processors: Grasses -->
    <range-processor name="harvest-grasses-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestGrassRangeProcessor"/>
    <range-processor name="harvest-grasses-moisture-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestGrassMoistureRangeProcessor"/>
    <range-processor name="harvest-grasses-panicledate-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestGrassPanicleDateRangeProcessor"/>
    <range-processor name="harvest-grasses-dryweight-processor" class="org.cabi.ofra.dataload.specific.HarvestTemplates$PlotHarvestGrassStoverDryRangeProcessor"/>
  </range-processors>
  <templates>
    <!-- Main Trial Definition Template -->
    <template name="trial-definition">
      <sheets>
        <sheet name="Trial" required="true" implementationClass="org.cabi.ofra.dataload.specific.TrialDefinitionTemplate$TrialSheetProcessor">
          <cells>
            <cell processorReference="valset" location="B23">
              <args>
                <arg name="variableName" value="trialUniqueId"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B2">
              <args>
                <arg name="variableName" value="countryCode"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B3">
              <args>
                <arg name="variableName" value="regionName"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B4">
              <args>
                <arg name="variableName" value="regionCode"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B5">
              <args>
                <arg name="variableName" value="districtName"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B6">
              <args>
                <arg name="variableName" value="districtCode"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B7">
              <args>
                <arg name="variableName" value="villageName"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B8">
              <args>
                <arg name="variableName" value="villageCode"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B9">
              <args>
                <arg name="variableName" value="cropOne"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B10">
              <args>
                <arg name="variableName" value="cropTwo"/>
                <arg name="required" value="false"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B11">
              <args>
                <arg name="variableName" value="cropThree"/>
                <arg name="required" value="false"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B12">
              <args>
                <arg name="variableName" value="year"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B13">
              <args>
                <arg name="variableName" value="season"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B15">
              <args>
                <arg name="variableName" value="farmerOrCentre"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B16">
              <args>
                <arg name="variableName" value="leadResearcher"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B17">
              <args>
                <arg name="variableName" value="fieldAssistantName"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B18">
              <args>
                <arg name="variableName" value="fieldAssistantTelephone"/>
                <arg name="toString" value="true"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B19">
              <args>
                <arg name="variableName" value="latitude"/>
              </args>
            </cell>
            <cell processorReference="valset" location="B20">
              <args>
                <arg name="variableName" value="longitude"/>
              </args>
            </cell>
          </cells>
        </sheet>
        <sheet name="Blocks" required="true" implementationClass="org.cabi.ofra.dataload.specific.TrialDefinitionTemplate$BlocksSheetProcessor">
          <cells>
            <cell processorReference="valset" location="B1">
              <args>
                <arg name="variableName" value="blocksTrialUID"/>
              </args>
            </cell>
          </cells>
          <ranges>
            <range start="A3" width="12" processorReference="trial-blocks-processor">
              <column-bindings>
                <column-binding column="0" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Block number must be specified in 'Blocks' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="1" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Latitude Point #1 must be specified in 'Blocks' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="2" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Longitude Point #1 must be specified in 'Blocks' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="9" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Elevation value must be specified in 'Blocks' sheet"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
        <sheet name="Plots" required="true" implementationClass="org.cabi.ofra.dataload.specific.TrialDefinitionTemplate$PlotsSheetProcessor">
          <cells>
            <cell processorReference="valset" location="B1">
              <args>
                <arg name="variableName" value="plotsTrialUID"/>
              </args>
            </cell>
          </cells>
          <ranges>
            <range start="A3" width="15" processorReference="trial-plots-processor">
              <column-bindings>
                <column-binding column="0" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Block number must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="1" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Plot number must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="2" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Plot area must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="3" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Nitrogen value for plot must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="4" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Phosphorus value for plot must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="5" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Potassium value for plot must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
                <column-binding column="11" processorReference="checkPresent">
                  <args>
                    <arg name="message" value="Crop One for plot must be specified in 'Plots' sheet"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Trial Soil Sample Template -->
    <template name="trial-soil-sample">
      <sheets>
        <sheet name="Trial Soil Sample" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="trial-soil-sample-processor" requireAll="true">
              <column-bindings>
                <column-binding column="0" processorReference="trialValidator">
                  <args>
                    <arg name="message" value="Cell value '%s' references trial unique identifier '%s', which does not exist"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Trial Soil Sample Result Template -->
    <template name="trial-soil-sample-result">
      <sheets>
        <sheet name="Trial Soil Result" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="18" processorReference="trial-soil-result-processor" requireAll="true">
              <column-bindings>
                <column-binding column="0" processorReference="trialSoilSampleValidator"/>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Block Activities Template -->
    <template name="block-activities">
      <sheets>
        <sheet name="Block-Activities" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="block-activities-processor">
              <column-bindings>
                <column-binding column="0" processorReference="blockValidator">
                  <args>
                    <arg name="trialMessage" value="Cell value %s references trial UID %s, which does not exist"/>
                    <arg name="blockMessage" value="Cell value %s references block %d for trial UID %s, which does not exist"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Block Observations Template -->
    <template name="block-observations">
      <sheets>
        <sheet name="Block-Observations" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="block-observations-processor">
              <column-bindings>
                <column-binding column="0" processorReference="blockValidator">
                  <args>
                    <arg name="trialMessage" value="Cell value %s references trial UID %s, which does not exist"/>
                    <arg name="blockMessage" value="Cell value %s references block %d for trial UID %s, which does not exist"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Block Soil Sample Template -->
    <template name="block-soil-sample">
      <sheets>
        <sheet name="Block Soil Sample" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="block-soil-sample-processor">
              <column-bindings>
                <column-binding column="0" processorReference="blockValidator">
                  <args>
                    <arg name="trialMessage" value="Cell value %s references trial UID %s, which does not exist"/>
                    <arg name="blockMessage" value="Cell value %s references block %d for trial UID %s, which does not exist"/>
                  </args>
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Block Soil Sample Result Template -->
    <template name="block-soil-sample-result">
      <sheets>
        <sheet name="Block Soil Result" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="block-soil-result-processor">
              <column-bindings>
                <column-binding column="0" processorReference="blockSoilSampleValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Block Weather Template -->
    <template name="block-weather">
      <sheets>
        <sheet name="Block Soil Sample" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="8" processorReference="block-weather-processor">
              <column-bindings>
                <column-binding column="0" processorReference="blockValidator"/>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Activities Template -->
    <template name="plot-activities">
      <sheets>
        <sheet name="Plot-Activities" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="plot-activities-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Observations Template -->
    <template name="plot-observations">
      <sheets>
        <sheet name="Plot-Observations" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="plot-observations-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Plant Sample Template -->
    <template name="plot-plant-sample">
      <sheets>
        <sheet name="Plot Plant Sample" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="4" processorReference="plot-plant-sample-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Plant Sample Result Template -->
    <template name="plot-plant-sample-result">
      <sheets>
        <sheet name="Plot Plant Result" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="14" processorReference="plot-plant-sample-results-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotPlantSampleValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Soil Sample Template -->
    <template name="plot-soil-sample">
      <sheets>
        <sheet name="Plot Soil Sample" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="4" processorReference="plot-soil-sample-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotValidator">
                </column-binding>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Plot Soil Sample Result Template -->
    <template name="plot-soil-sample-result">
      <sheets>
        <sheet name="Plot Soil Result" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="14" processorReference="plot-soil-sample-results-processor">
              <column-bindings>
                <column-binding column="0" processorReference="plotSoilSampleValidator"/>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Beans Harvesting Template -->
    <template name="plot-harvest-beans">
      <sheets>
        <sheet name="Beans Harvest" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="5" processorReference="harvest-legumes-processor" requireAll="true">
              <column-bindings>
                <column-binding column="0" processorReference="plotValidator"/>
              </column-bindings>
            </range>
          </ranges>
        </sheet>
      </sheets>
    </template>
    <!-- Beans Moisture Content Template -->
    <template name="plot-harvest-beans-moisture">
