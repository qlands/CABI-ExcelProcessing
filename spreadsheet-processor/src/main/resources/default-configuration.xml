    <!-- Maize Silk Date Template -->
    <template name="plot-harvest-maize-silkdate">
      <sheets>
        <sheet name="Maize Silk Date" required="true" implementationClass="org.cabi.ofra.dataload.impl.BaseSheetProcessor">
          <ranges>
            <range start="A2" width="2" processorReference="harvest-maize-silkdate-processor" requireAll="true">
              <column-bindings>
